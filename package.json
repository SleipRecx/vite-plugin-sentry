    "@typescript-eslint/parser": "5.5.0",
    "eslint": "8.3.0",
    "jest": "27.4.2",
    "prettier": "2.5.0",
    "rollup": "2.60.2",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-typescript2": "0.31.1",
    "typescript": "4.5.2"
